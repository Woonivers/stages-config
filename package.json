{
  "name": "@woonivers/stages-config",
  "version": "1.0.2",
  "description": "Easy environment by stages",
  "repository": {
    "type": "git",
    "url": "https://github.com/Woonivers/stages-config.git"
  },
  "main": "lib/index.js",
  "typings": "lib/index.d.ts",
  "files": ["lib/"],
  "scripts": {
    "test": "jest",
    "lint": "eslint 'src/**/*.{ts,tsx}'",
    "format": "prettier --write 'src/**/*.{ts,tsx}'",
    "build": "rm -rf lib && tsc -p .",
    "prepublishOnly": "npm run build"
  },
  "keywords": ["stages", "config-module"],
  "author": "Jorge Marrero",
  "license": "MIT",
  "jest": {
    "preset": "ts-jest",
    "testEnvironment": "node"
  },
  "lint-staged": {
    "*.ts": ["prettier --write", "tslint --fix", "git add"],
    "*.md": ["prettier --write", "git add"],
    "*.json": ["prettier --write", "git add"]
  },
  "dependencies": {
    "eslint-import-resolver-typescript": "^1.1.1",
    "lodash": "^4.17.11",
    "validator": "^10.11.0"
  },
  "devDependencies": {
    "@commitlint/cli": "^7.5.2",
    "@commitlint/config-conventional": "^7.5.0",
    "@types/jest": "^24.0.9",
    "@types/lodash": "^4.14.121",
    "@types/node": "^11.10.4",
    "@types/validator": "^10.9.0",
    "@typescript-eslint/eslint-plugin": "^1.4.2",
    "@typescript-eslint/parser": "^1.4.2",
    "eslint": "^5.15.0",
    "eslint-config-airbnb-base": "^13.1.0",
    "eslint-config-prettier": "^4.1.0",
    "eslint-config-typescript": "^2.0.0",
    "eslint-plugin-import": "^2.16.0",
    "husky": "^1.3.1",
    "jest": "^24.1.0",
    "lint-staged": "^8.1.5",
    "prettier": "^1.16.4",
    "ts-jest": "^24.0.0",
    "ts-node": "^8.0.2",
    "typescript": "3.2.2",
    "typescript-eslint-parser": "^22.0.0"
  }
}
